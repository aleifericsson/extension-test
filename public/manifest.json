{
    "manifest_version": 3,
    "name": "wingAI",
    "icons":{
        "64":"wingai.png"
    },
    "version": "1.0.0",
    "background":{
        "service_worker":"service-worker.js",
        "type":"module"
    },
    "action":{
      "default_popup":"index.html"
    },
    "permissions": [
      "scripting"
    ],
    "host_permissions":[
      "http://*/*",
      "https://*/*"
    ]
  }